{
  "name": "@superbia/client",
  "version": "2.4.0",
  "description": "Connect to a Superbia server.",
  "homepage": "https://superbia.dev",
  "repository": {
    "type": "git",
    "url": "https://github.com/iconshot/superbia-client.git"
  },
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/cjs/index.js",
      "import": "./dist/esm/index.js"
    }
  },
  "scripts": {
    "clean": "rm -rf dist",
    "build-cjs": "tsc --project tsconfig.cjs.json",
    "build-esm": "tsc --project tsconfig.esm.json",
    "build": "npm run clean && npm run build-cjs && npm run build-esm",
    "prepare": "npm run build"
  },
  "keywords": [
    "superbia",
    "client"
  ],
  "license": "MIT",
  "devDependencies": {
    "typescript": "^5.6.3"
  },
  "dependencies": {
    "everemitter": "^1.4.1",
    "tsion": "^1.0.1"
  }
}
